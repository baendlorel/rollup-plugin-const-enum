		"import": "./dist/index.mjs",
		"require": "./dist/index.cjs",
		"default": "./dist/index.mjs"
	},
	"files": [
		"dist"
	],
	"homepage": "https://github.com/baendlorel/rollup-plugin-const-enum#readme",
